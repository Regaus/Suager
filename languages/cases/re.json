{  // Pattern[number][case], default ~ singular, default ~ nominative
  "^[Kk][aä]in(?:ea|ee|ää)d$": {  // kainead - C2
    "default": {
      "default": "<>",              // kainead / Mu kaineed / Kv käinääd
      "nominative": "<>",           // kainead / Mu kaineed / Kv käinääd
      "accusative": "<:-3>edan",    // kainedan
      "genitive": "<:-3>edan",      // kainedan
      "dative": "<:-3>edut",        // kainedut
      "instrumental": "<:-3>edur",  // kainedur
      "locative": "<:-3>edi"        // kainedi
    },
    "plural": {
      "default": "<:-3>edat",        // kainedat
      "nominative": "<:-3>edat",     // kainedat
      "accusative": "<:-3>edaan",    // kainedaan
      "genitive": "<:-3>edain",      // kainedain
      "dative": "<:-3>edaat",        // kainedaat
      "instrumental": "<:-3>edaar",  // kainedaar
      "locative": "<:-3>edai"        // kainedai
    }
  },
  "^(?!(.+)var$)(([Rr]egaus|[Ll]ias)|(.+)[bcdfghjklmnprtvwxz])$": {  // Regaus, lias (s-decl. exceptions) | C1 | Does NOT match -var endings
    "default": {
      "default": "<>",         // kad
      "nominative": "<>",      // kad
      "accusative": "<>an",    // kadan
      "genitive": "<>an",      // kadan
      "dative": "<>ut",        // kadut
      "instrumental": "<>ur",  // kadur
      "locative": "<>i"        // kadi
    },
    "plural": {
      "default": "<>at",        // kadat
      "nominative": "<>at",     // kadat
      "accusative": "<>aan",    // kadaan
      "genitive": "<>ain",      // kadain
      "dative": "<>aat",        // kadaat
      "instrumental": "<>aar",  // kadaar
      "locative": "<>ai"        // kadai
    }
  },
  "^[Ss]aa$": {  // saa - A2
    "default": {
      "default": "<>",        // saa
      "nominative": "<>",     // saa
      "accusative": "<>n",    // saan
      "genitive": "<>n",      // saan
      "dative": "<>t",        // saat
      "instrumental": "<>r",  // saar
      "locative": "<:-1>i"    // sai
    },
    "plural": {
      "default": "<>t",        // saat
      "nominative": "<>t",     // saat
      "accusative": "<>an",    // saaan
      "genitive": "<>in",      // saain
      "dative": "<>at",        // saaat
      "instrumental": "<>ar",  // saaar
      "locative": "<>i"        // saai
    }
  },
  "^(.+)aa$": {  // -aa - A3
    "default": {
      "default": "<>",        // vahtaa
      "nominative": "<>",     // vahtaa
      "accusative": "<>n",    // vahtaan
      "genitive": "<>n",      // vahtaan
      "dative": "<>t",        // vahtaat
      "instrumental": "<>r",  // vahtaar
      "locative": "<:-1>i"    // vahtai
    },
    "plural": {
      "default": "<>t",        // vahtaat
      "nominative": "<>t",     // vahtaat
      "accusative": "<>n",     // vahtaan
      "genitive": "<:-1>in",   // vahtain
      "dative": "<>t",         // vahtaat
      "instrumental": "<>r",   // vahtaar
      "locative": "<:-1>ji"    // vahtaji
    }
  },
  "^(.+)aas$": {  // -aas - A3 with s
    "default": {
      "default": "<>",           // Raas
      "nominative": "<>",        // Raas
      "accusative": "<:-1>n",    // Raan
      "genitive": "<:-1>n",      // Raan
      "dative": "<:-1>t",        // Raat
      "instrumental": "<:-1>r",  // Raar
      "locative": "<:-2>i"       // Rai
    },
    "plural": {
      "default": "<:-1>t",        // Raat
      "nominative": "<:-1>t",     // Raat
      "accusative": "<:-1>n",     // Raan
      "genitive": "<:-1>n",       // Raan
      "dative": "<:-1>t",         // Raat
      "instrumental": "<:-1>r",   // Raar
      "locative": "<:-1>ji"       // Raaji
    }
  },
  "^(.+)[aä]$": {  // -a - A1 | -ä - Ä1 (same pattern)
    "default": {
      "default": "<>",        // sea
      "nominative": "<>",     // sea
      "accusative": "<>n",    // sean
      "genitive": "<>n",      // sean
      "dative": "<>t",        // seat
      "instrumental": "<>r",  // sear
      "locative": "<:-1>i"    // sei
    },
    "plural": {
      "default": "<>t",        // seat
      "nominative": "<>t",     // seat
      "accusative": "<>an",    // seaan
      "genitive": "<>in",      // seain
      "dative": "<>at",        // seaat
      "instrumental": "<>ar",  // seaar
      "locative": "<>i"        // seai
    }
  },
  "^(.+)([aä]s|var)$": {  // -as - A1 | -äs - Ä1 with s | -var - month endings
    "default": {
      "default": "<>",           // lias
      "nominative": "<>",        // lias
      "accusative": "<:-1>n",    // lian
      "genitive": "<:-1>n",      // lian
      "dative": "<:-1>t",        // liat
      "instrumental": "<:-1>r",  // liar
      "locative": "<:-2>i"       // lii
    },
    "plural": {
      "default": "<:-1>t",        // liat
      "nominative": "<:-1>t",     // liat
      "accusative": "<:-1>an",    // liaan
      "genitive": "<:-1>in",      // liain
      "dative": "<:-1>at",        // liaat
      "instrumental": "<:-1>ar",  // liaar
      "locative": "<:-1>i"        // liai
    }
  },
  "^(?![Dd]erge)((.+)[eouö])$": {  // -e - E1 | -o - O1 | -u - U1 | -ö - Ö1 | Matches if the value is NOT "derge"
    "default": {
      "default": "<>",        // penteme
      "nominative": "<>",     // penteme
      "accusative": "<>n",    // pentemen
      "genitive": "<>n",      // pentemen
      "dative": "<>t",        // pentemet
      "instrumental": "<>r",  // pentemer
      "locative": "<:-1>i"    // pentemi
    },
    "plural": {
      "default": "<:-1>at",        // pentemat
      "nominative": "<:-1>at",     // pentemat
      "accusative": "<:-1>aan",    // pentemaan
      "genitive": "<:-1>ain",      // pentemain
      "dative": "<:-1>aat",        // pentemaat
      "instrumental": "<:-1>aar",  // pentemaar
      "locative": "<:-1>ai"        // pentemai
    }
  },
  "^((.+)[eouö]s|[Ii]latis|[Ii]skalis)$": {  // -e - E1 | -o - O1 | -u - U1 | -ö - Ö1 with s | ilatis, iskalis - I1
    "default": {
      "default": "<>",           // veises
      "nominative": "<>",        // veises
      "accusative": "<:-1>n",    // veisen
      "genitive": "<:-1>n",      // veisen
      "dative": "<:-1>t",        // veiset
      "instrumental": "<:-1>r",  // veiser
      "locative": "<:-1>i"       // veisei
    },
    "plural": {
      "default": "<:-2>at",        // veisat
      "nominative": "<:-2>at",     // veisat
      "accusative": "<:-2>aan",    // veisain
      "genitive": "<:-2>ain",      // veisain
      "dative": "<:-2>aat",        // veisaat
      "instrumental": "<:-2>aar",  // veisaar
      "locative": "<:-2>ai"        // veisai
    }
  },
  "^[Ll]eitakis$": {  // leitakis - I3 (-jat plural)
    "default": {
      "default": "<>",           // leitakis
      "nominative": "<>",        // leitakis
      "accusative": "<:-1>n",    // leitakin
      "genitive": "<:-1>n",      // leitakin
      "dative": "<:-1>t",        // leitakit
      "instrumental": "<:-1>r",  // leitakir
      "locative": "<:-1>i"       // leitakii
    },
    "plural": {
      "default": "<:-3>jat",        // leitajat
      "nominative": "<:-3>jat",     // leitajat
      "accusative": "<:-3>jaan",    // leitajaan
      "genitive": "<:-3>jain",      // leitajain
      "dative": "<:-3>jaat",        // leitajaat
      "instrumental": "<:-3>jaar",  // leitajaar
      "locative": "<:-3>jai"        // leitajai
    }
  },
  "^(.+)[aeiouäyöü]i$": {  // -Vi - I4 (koi, nei)
    "default": {
      "default": "<>",        // nei
      "nominative": "<>",     // neis
      "accusative": "<>n",    // nein
      "genitive": "<>n",      // nein
      "dative": "<>t",        // neit
      "instrumental": "<>r",  // neir
      "locative": "<>ji"      // neiji
    },
    "plural": {
      "default": "<>jat",        // neijat
      "nominative": "<>jat",     // neijat
      "accusative": "<>jaan",    // neijaan
      "genitive": "<>jaan",      // neijaan
      "dative": "<>jaat",        // neijaat
      "instrumental": "<>jaar",  // neijaar
      "locative": "<>jai"        // neijai
    }
  },
  "^([Dd]erge|(.+)[iü])$": {  // derge - E2 | -i - I2 (vintelis) | -ü - Ü1
    "default": {
      "default": "<>",        // derge
      "nominative": "<>",     // derge
      "accusative": "<>n",    // dergen
      "genitive": "<>n",      // dergen
      "dative": "<>t",        // derget
      "instrumental": "<>r",  // derger
      "locative": "<:-1>i"    // dergi
    },
    "plural": {
      "default": "<:-1>ät",        // dergät
      "nominative": "<:-1>ät",     // dergät
      "accusative": "<:-1>äan",    // dergäan
      "genitive": "<:-1>äin",      // dergäin
      "dative": "<:-1>äat",        // dergäat
      "instrumental": "<:-1>äar",  // dergäar
      "locative": "<:-1>äi"        // dergäi
    }
  },
  "^(.+)[iü]s$": {  // -i - I2 (vintelis) | -ü - Ü1 with s
    "default": {
      "default": "<>",           // vintelis
      "nominative": "<>",        // vintelis
      "accusative": "<:-1>n",    // vintelin
      "genitive": "<:-1>n",      // vintelin
      "dative": "<:-1>t",        // vintelit
      "instrumental": "<:-1>r",  // vintelir
      "locative": "<:-1>i"       // vintelii
    },
    "plural": {
      "default": "<:-2>ät",        // vintelät
      "nominative": "<:-2>ät",     // vintelät
      "accusative": "<:-2>äan",    // vinteläan
      "genitive": "<:-2>äin",      // vinteläin
      "dative": "<:-2>äat",        // vinteläat
      "instrumental": "<:-2>äar",  // vinteläar
      "locative": "<:-2>äi"        // vinteläi
    }
  },
  "^.+": {  // "Default" in case no pattern matches at all
    "default": {
      "default": "<>",
      "nominative": "<>",
      "accusative": "<>'n",
      "genitive": "<>'n",
      "dative": "<>'t",
      "instrumental": "<>'r",
      "locative": "<>'i"
    },
    "plural": {
      "default": "<>'at",
      "nominative": "<>'at",
      "accusative": "<>'aan",
      "genitive": "<>'ain",
      "dative": "<>'aat",
      "instrumental": "<>'aar",
      "locative": "<>'ai"
    }
  }
}