{  // Pattern[number][case], default ~ singular, default ~ nominative
  "^kainead$": {  // kainead - C2
    "default": {
      "default": "<>",             // kainead
      "nominative": "<>",          // kainead
      "accusative": "<:-2>dan",    // kainedan
      "genitive": "<:-2>dan",      // kainedan
      "dative": "<:-2>dut",        // kainedut
      "instrumental": "<:-2>dur"   // kainedur
    },
    "plural": {
      "default": "<:-2>dat",        // kainedat
      "nominative": "<:-2>dat",     // kainedat
      "accusative": "<:-2>daan",    // kainedaan
      "genitive": "<:-2>dain",      // kainedain
      "dative": "<:-2>daat",        // kainedaat
      "instrumental": "<:-2>daar"   // kainedaar
    }
  },
  "^(?!(.+)var$)(([Rr]egaus)|(.+)[bcdfghjklmnprtvwxz])$": {  // Regaus, Leitoxz (s-decl. exceptions) | C1 | Does NOT match -var endings
    "default": {
      "default": "<>",         // kad
      "nominative": "<>",      // kad
      "accusative": "<>an",    // kadan
      "genitive": "<>an",      // kadan
      "dative": "<>ut",        // kadut
      "instrumental": "<>ur"   // kadur
    },
    "plural": {
      "default": "<>at",        // kadat
      "nominative": "<>at",     // kadat
      "accusative": "<>aan",    // kadaan
      "genitive": "<>ain",      // kadain
      "dative": "<>aat",        // kadaat
      "instrumental": "<>aar"   // kadaar
    }
  },
  "^saa$": {  // saa - A2
    "default": {
      "default": "<>",        // saa
      "nominative": "<>",     // saa
      "accusative": "<>n",    // saan
      "genitive": "<>n",      // saan
      "dative": "<>t",        // saat
      "instrumental": "<>r"   // saar
    },
    "plural": {
      "default": "<>t",        // saat
      "nominative": "<>t",     // saat
      "accusative": "<>an",    // saaan
      "genitive": "<>in",      // saain
      "dative": "<>at",        // saaat
      "instrumental": "<>ar"   // saaar
    }
  },
  "^(.+)aa$": {  // -aa - A3
    "default": {
      "default": "<>",        // vahtaa
      "nominative": "<>",     // vahtaa
      "accusative": "<>n",    // vahtaan
      "genitive": "<>n",      // vahtaan
      "dative": "<>t",        // vahtaat
      "instrumental": "<>r"   // vahtaar
    },
    "plural": {
      "default": "<>t",        // vahtaat
      "nominative": "<>t",     // vahtaat
      "accusative": "<>n",     // vahtaan
      "genitive": "<:-1>in",   // vahtain
      "dative": "<>t",         // vahtaat
      "instrumental": "<>r"    // vahtaar
    }
  },
  "^(.+)aas$": {  // -aas - A3 with s
    "default": {
      "default": "<>",           // Raas
      "nominative": "<>",        // Raas
      "accusative": "<:-1>n",    // Raan
      "genitive": "<:-1>n",      // Raan
      "dative": "<:-1>t",        // Raat
      "instrumental": "<:-1>r"   // Raar
    },
    "plural": {
      "default": "<:-1>t",        // Raat
      "nominative": "<:-1>t",     // Raat
      "accusative": "<:-1>n",     // Raan
      "genitive": "<:-1>n",       // Raan
      "dative": "<:-1>t",         // Raat
      "instrumental": "<:-1>r"    // Raar
    }
  },
  "^(.+)[aä]$": {  // -a - A1 | -ä - Ä1 (same pattern)
    "default": {
      "default": "<>",        // sea
      "nominative": "<>",     // sea
      "accusative": "<>n",    // sean
      "genitive": "<>n",      // sean
      "dative": "<>t",        // seat
      "instrumental": "<>r"   // sear
    },
    "plural": {
      "default": "<>t",        // seat
      "nominative": "<>t",     // seat
      "accusative": "<>an",    // seaan
      "genitive": "<>in",      // seain
      "dative": "<>at",        // seaat
      "instrumental": "<>ar"   // seaar
    }
  },
  "^(.+)([aä]s|var)$": {  // -as - A1 | -äs - Ä1 with s | -var - month endings
    "default": {
      "default": "<>",           // lias
      "nominative": "<>",        // lias
      "accusative": "<:-1>n",    // lian
      "genitive": "<:-1>n",      // lian
      "dative": "<:-1>t",        // liat
      "instrumental": "<:-1>r"   // liar
    },
    "plural": {
      "default": "<:-1>t",        // liat
      "nominative": "<:-1>t",     // liat
      "accusative": "<:-1>an",    // liaan
      "genitive": "<:-1>in",      // liain
      "dative": "<:-1>at",        // liaat
      "instrumental": "<:-1>ar"   // liaar
    }
  },
  "^(?!derge)((.+)[eouö])$": {  // -e - E1 | -o - O1 | -u - U1 | -ö - Ö1 | Matches if the value is NOT "derge"
    "default": {
      "default": "<>",        // penteme
      "nominative": "<>",     // penteme
      "accusative": "<>n",    // pentemen
      "genitive": "<>n",      // pentemen
      "dative": "<>t",        // pentemet
      "instrumental": "<>r"   // pentemer
    },
    "plural": {
      "default": "<:-1>at",        // pentemat
      "nominative": "<:-1>at",     // pentemat
      "accusative": "<:-1>aan",    // pentemaan
      "genitive": "<:-1>ain",      // pentemain
      "dative": "<:-1>aat",        // pentemaat
      "instrumental": "<:-1>aar"   // pentemaar
    }
  },
  "^((.+)[eouö]s|ilatis)$": {  // -e - E1 | -o - O1 | -u - U1 | -ö - Ö1 with s | ilatis - I1
    "default": {
      "default": "<>",           // veises
      "nominative": "<>",        // veises
      "accusative": "<:-1>n",    // veisen
      "genitive": "<:-1>n",      // veisen
      "dative": "<:-1>t",        // veiset
      "instrumental": "<:-1>r"   // veiser
    },
    "plural": {
      "default": "<:-2>at",        // veisat
      "nominative": "<:-2>at",     // veisat
      "accusative": "<:-2>aan",    // veisain
      "genitive": "<:-2>ain",      // veisain
      "dative": "<:-2>aat",        // veisaat
      "instrumental": "<:-2>aar"   // veisaar
    }
  },
  "^leitakis$": {  // leitakis - I3 (-jat plural)
    "default": {
      "default": "<>",           // leitakis
      "nominative": "<>",        // leitakis
      "accusative": "<:-1>n",    // leitakin
      "genitive": "<:-1>n",      // leitakin
      "dative": "<:-1>t",        // leitakit
      "instrumental": "<:-1>r"   // leitakir
    },
    "plural": {
      "default": "<:-3>jat",        // leitajat
      "nominative": "<:-3>jat",     // leitajat
      "accusative": "<:-3>jaan",    // leitajaan
      "genitive": "<:-3>jain",      // leitajain
      "dative": "<:-3>jaat",        // leitajaat
      "instrumental": "<:-3>jaar"   // leitajaar
    }
  },
  "^(.+)[aeiouäyöü]i$": {  // -Vi - I4 (koi, nei)
    "default": {
      "default": "<>",        // nei
      "nominative": "<>",     // neis
      "accusative": "<>n",    // nein
      "genitive": "<>n",      // nein
      "dative": "<>t",        // neit
      "instrumental": "<>r"   // neir
    },
    "plural": {
      "default": "<>jat",        // neijat
      "nominative": "<>jat",     // neijat
      "accusative": "<>jaan",    // neijaan
      "genitive": "<>jaan",      // neijaan
      "dative": "<>jaat",        // neijaat
      "instrumental": "<>jaar"   // neijaar
    }
  },
  "^(derge|(.+)[iü])$": {  // derge - E2 | -i - I2 (vintelis) | -ü - Ü1
    "default": {
      "default": "<>",        // derge
      "nominative": "<>",     // derge
      "accusative": "<>n",    // dergen
      "genitive": "<>n",      // dergen
      "dative": "<>t",        // derget
      "instrumental": "<>r"   // derger
    },
    "plural": {
      "default": "<:-1>ät",        // dergät
      "nominative": "<:-1>ät",     // dergät
      "accusative": "<:-1>äan",    // dergäan
      "genitive": "<:-1>äin",      // dergäin
      "dative": "<:-1>äat",        // dergäat
      "instrumental": "<:-1>äar"   // dergäar
    }
  },
  "^(.+)[iü]s$": {  // -i - I2 (vintelis) | -ü - Ü1 with s
    "default": {
      "default": "<>",           // vintelis
      "nominative": "<>",        // vintelis
      "accusative": "<:-1>n",    // vintelin
      "genitive": "<:-1>n",      // vintelin
      "dative": "<:-1>t",        // vintelit
      "instrumental": "<:-1>r"   // vintelir
    },
    "plural": {
      "default": "<:-2>ät",        // vintelät
      "nominative": "<:-2>ät",     // vintelät
      "accusative": "<:-2>äan",    // vinteläan
      "genitive": "<:-2>äin",      // vinteläin
      "dative": "<:-2>äat",        // vinteläat
      "instrumental": "<:-2>äar"   // vinteläar
    }
  },
  "^.+": {  // "Default" in case no pattern matches at all
    "default": {
      "default": "<>",
      "nominative": "<>",
      "accusative": "<>'n",
      "genitive": "<>'n",
      "dative": "<>'t",
      "instrumental": "<>'r"
    },
    "plural": {
      "default": "<>'at",
      "nominative": "<>'at",
      "accusative": "<>'aan",
      "genitive": "<>'ain",
      "dative": "<>'aat",
      "instrumental": "<>'aar"
    }
  }
}