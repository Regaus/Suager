      "locative": "<:-2>ēvi",      // -ēvi
      "ablative": "<:-2>o",        // -o
      "instrumental": "<:-2>olō",  // -olō
      "comitative": "<:-2>ocum"    // -ocum
    },
    "plural": {
      "default": "<:-2>ē",          // -ē
      "vocative": "<:-2>ē",         // -ē
      "accusative": "<:-2>ocs",     // -ocs
      "genitive": "<:-2>ōro",       // -ōro
      "dative": "<:-2>ōis",         // -ōis
      "locative": "<:-2>ōsvi",      // -ōsvi
      "ablative": "<:-2>ōdsi",      // -ōdsi
      "instrumental": "<:-2>ōlos",  // -ōlos
      "comitative": "<:-2>ocuns"    // -ocuns
    }
  },
  "^.+io$": {  // -io - 2N
    "default": {
      "default": "<>",         // -io
      "vocative": "<>",        // -io
      "accusative": "<>",      // -io
      "genitive": "<:-1>ī",    // -iī
      "dative": "<:-1>ē",      // -iē
      "locative": "<:-2>īvi",  // -īvi
      "ablative": "<>",        // -io
      "instrumental": "<>lō",  // -iolō
      "comitative": "<>cum"    // -iocum
    },
    "plural": {
      "default": "<:-1>ū",          // -iū
      "vocative": "<:-1>ū",         // -iū
      "accusative": "<:-1>ū",       // -iū
      "genitive": "<:-1>ōro",       // -iōro
      "dative": "<:-1>ōis",         // -iōis
      "locative": "<:-1>ōsvi",      // -iōsvi
      "ablative": "<:-1>ōdsi",      // -iōdsi
      "instrumental": "<:-1>ōlos",  // -iōlos
      "comitative": "<>cuns"        // -iocuns
    }
  },
  "^.+o$": {  // -o - 2N
    "default": {
      "default": "<>",         // -o
      "vocative": "<>",        // -o
      "accusative": "<>",      // -o
      "genitive": "<:-1>ī",    // -ī
      "dative": "<:-1>ē",      // -ē
      "locative": "<:-1>ēvi",  // -ēvi
      "ablative": "<>",        // -o
      "instrumental": "<>lō",  // -olō
      "comitative": "<>cum"    // -ocum
    },
    "plural": {
      "default": "<:-1>ū",          // -ū
      "vocative": "<:-1>ū",         // -ū
      "accusative": "<:-1>ū",       // -ū
      "genitive": "<:-1>ōro",       // -ōro
      "dative": "<:-1>ōis",         // -ōis
      "locative": "<:-1>ōsvi",      // -ōsvi
      "ablative": "<:-1>ōdsi",      // -ōdsi
      "instrumental": "<:-1>ōlos",  // -ōlos
      "comitative": "<>cuns"        // -ocuns
    }
  },
  "^.+is$": {  // -is - 3F
    "default": {
      "default": "<>",           // -is
      "vocative": "<>",          // -is
      "accusative": "<:-1>",     // -i
      "genitive": "<:-2>īs",     // -īs
      "dative": "<:-2>ī",        // -ī
      "locative": "<:-2>īvi",    // -īvi
      "ablative": "<:-1>",       // -i
      "instrumental": "<:-1>l",  // -il
      "comitative": "<:-1>cum"   // -icum
    },
    "plural": {
      "default": "<:-2>es",        // -es
      "vocative": "<:-2>es",       // -es
      "accusative": "<:-2>ecs",    // -ecs
      "genitive": "<:-1>o",        // -io
      "dative": "<:-1>bus",        // -ibus
      "locative": "<:-2>īsvi",     // -īsvi
      "ablative": "<:-2>īdsi",     // -īdsi
      "instrumental": "<:-2>īlī",  // -īlī
      "comitative": "<:-1>cuns"    // -icuns
    }
  },
  "^.+i$": {  // -i - 3N
    "default": {
      "default": "<>",         // -i
      "vocative": "<>",        // -i
      "accusative": "<>",      // -i
      "genitive": "<:-1>īs",   // -īs
      "dative": "<:-1>ī",      // -ī
      "locative": "<:-1>īvi",  // -īvi
      "ablative": "<>",        // -i
      "instrumental": "<>l",   // -il
      "comitative": "<>cum"    // -icum
    },
    "plural": {
      "default": "<>ū",            // -iū
      "vocative": "<>ū",           // -iū
      "accusative": "<>ū",         // -iū
      "genitive": "<>o",           // -io
      "dative": "<>bus",           // -ibus
      "locative": "<:-1>īsvi",     // -īsvi
      "ablative": "<:-1>īdsi",     // -īdsi
      "instrumental": "<:-1>īlī",  // -īlī
      "comitative": "<>cuns"       // -icuns
    }
  },
  "^.+us4$": {  // -us - 4M -- Add 4 to end of word to specify 4th declension
    "default": {
      "default": "<:-1>",        // -us
      "vocative": "<:-2>",       // -u
      "accusative": "<:-2>",     // -u
      "genitive": "<:-3>vis",    // -vis
      "dative": "<:-2>vī",       // -uvī
      "locative": "<:-2>vū",     // -uvū
      "ablative": "<:-2>",       // -u
      "instrumental": "<:-2>l",  // -ul
      "comitative": "<:-2>cum"   // -ucum
    },
    "plural": {
      "default": "<:-2>vis",       // -uvis
      "vocative": "<:-2>vis",      // -uvis
      "accusative": "<:-3>ocs",    // -ocs
      "genitive": "<:-3>vo",       // -vo
      "dative": "<:-2>bus",        // -ubus
      "locative": "<:-3>ūsvi",     // -ūsvi
      "ablative": "<:-3>ūdsi",     // -ūdsi
      "instrumental": "<:-3>ūlū",  // -ūlū
      "comitative": "<:-2>cuns"    // -ucuns
    }
  },
  "^.+ū$": {  // -ū - 4F
    "default": {
      "default": "<>",            // -ū
      "vocative": "<:-1>u",       // -u
      "accusative": "<:-1>u",     // -u
      "genitive": "<:-1>vis",     // -vis
      "dative": "<:-1>uvī",       // -uvī
      "locative": "<:-1>uvū",     // -uvū
      "ablative": "<:-1>u",       // -u
      "instrumental": "<:-1>ul",  // -ul
      "comitative": "<:-1>ucum"   // -ucum
    },
    "plural": {
      "default": "<:-1>uva",      // -uva
      "vocative": "<:-1>uva",     // -uva
      "accusative": "<:-1>ocs",   // -ocs
      "genitive": "<:-1>vo",      // -vo
      "dative": "<:-1>ubus",      // -ubus
      "locative": "<>svi",        // -ūsvi
      "ablative": "<>dsi",        // -ūdsi
      "instrumental": "<>lū",     // -ūlū
      "comitative": "<:-1>ucuns"  // -ucuns
    }
  },
  "^.+u$": {  // -u - 4N
    "default": {
      "default": "<>",         // -u
      "vocative": "<>",        // -u
      "accusative": "<>",      // -u
      "genitive": "<:-1>vis",  // -vis
      "dative": "<>vī",        // -uvī
      "locative": "<>vū",      // -uvū
      "ablative": "<>",        // -u
      "instrumental": "<>l",   // -ul
      "comitative": "<>cum"    // -ucum
    },
    "plural": {
      "default": "<:-1>ū",         // -ū
      "vocative": "<:-1>ū",        // -ū
      "accusative": "<:-1>ū",      // -ū
      "genitive": "<:-1>vo",       // -vo
      "dative": "<>bus",           // -ubus
      "locative": "<:-1>ūsvi",     // -ūsvi
      "ablative": "<:-1>ūdsi",     // -ūdsi
      "instrumental": "<:-1>ūlū",  // -ūlū
      "comitative": "<>cuns"       // -ucuns
    }
  },
  "^.+s$": {  // -Cs - 5M/F -- Allow anything before the s, assume there are no exceptions
    "default": {
      "default": "<>",            // -s
      "vocative": "<>",           // -s
      "accusative": "<:-1>e",     // -e
      "genitive": "<:-1>us",      // -us
      "dative": "<:-1>ī",         // -ī
      "locative": "<:-1>yvi",     // -yvi
      "ablative": "<:-1>a",       // -a
      "instrumental": "<:-1>ol",  // -ol
      "comitative": "<:-1>ocum"   // -ocum
    },
    "plural": {
      "default": "<:-1>is",         // -is
      "vocative": "<:-1>is",        // -is
      "accusative": "<:-1>ecs",     // -ecs
      "genitive": "<:-1>om",        // -om
      "dative": "<:-1>ebus",        // -ebus
      "locative": "<:-1>ēsvi",      // -ēsvi
      "ablative": "<:-1>āra",       // -āra
      "instrumental": "<:-1>ōles",  // -ōles
      "comitative": "<:-1>ocuns"    // -ocuns
    }
  },
  "^.+": {  // 5N -- Assume that all words that don't match any other declension are 5N, although it should check for it ending on a consonant
    "default": {
      "default": "<>",         // -
      "vocative": "<>",        // -
      "accusative": "<>",      // -
      "genitive": "<>us",      // -us
      "dative": "<>ī",         // -ī
      "locative": "<>yvi",     // -yvi
      "ablative": "<>a",       // -a
      "instrumental": "<>ol",  // -ol
      "comitative": "<>ocum"   // -ocum
    },
    "plural": {
      "default": "<>ū",          // -ū
      "vocative": "<>ū",         // -ū
      "accusative": "<>ū",       // -ū
      "genitive": "<>om",        // -om
      "dative": "<>ebus",        // -ebus
      "locative": "<>ēsvi",      // -ēsvi
      "ablative": "<>āra",       // -āra
      "instrumental": "<>ōles",  // -ōles
      "comitative": "<>ocuns"    // -ocuns
    }
  }
