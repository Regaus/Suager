{
  "^.+ā$": {  // -ā - 1
    "default": {
      "default": "<>",            // -ā
      "vocative": "<>",           // -ā
      "accusative": "<:-1>a",     // -a
      "genitive": "<>n",          // -ān
      "dative": "<:-1>ē",         // -ē
      "locative": "<>vi",         // -āvi
      "ablative": "<:-1>a",       // -a
      "instrumental": "<:-1>al",  // -al
      "comitative": "<>cum"       // -ācum
    },
    "plural": {
      "default": "<:-1>as",      // -as
      "vocative": "<:-1>as",     // -as
      "accusative": "<:-1>acs",  // -acs
      "genitive": "<>ren",       // -āren
      "dative": "<>is",          // -āis
      "locative": "<>svi",       // -āsvi
      "ablative": "<>dsi",       // -ādsi
      "instrumental": "<>alsi",  // -ālsi
      "comitative": "<>cuns"     // -ācuns
    }
  },
  "^.+ius$": {  // -ius - 2M
    "default": {
      "default": "<>",             // -ius
      "vocative": "<:-3>ī",        // -ī
      "accusative": "<:-2>o",      // -io
