      "dative": "<:-2>ē",          // -iē
      "locative": "<:-3>īvi",      // -īvi
      "ablative": "<:-2>o",        // -io
      "instrumental": "<:-2>olō",  // -iolō
      "comitative": "<:-2>ocum"    // -iocum
    },
    "plural": {
      "default": "<:-2>ē",          // -iē
      "vocative": "<:-2>ē",         // -iē
      "accusative": "<:-2>ocs",     // -iocs
      "genitive": "<:-2>ōro",       // -iōro
      "dative": "<:-2>ōis",         // -iōis
      "locative": "<:-2>ōsvi",      // -iōsvi
      "ablative": "<:-2>ōdsi",      // -iōdsi
      "instrumental": "<:-2>ōlos",  // -iōlos
      "comitative": "<:-2>ocuns"    // -iocuns
    }
  },
  "^.+us$": {  // -us - 2M
    "default": {
      "default": "<>",             // -us
      "vocative": "<:-2>i",        // -i
      "accusative": "<:-2>o",      // -o
